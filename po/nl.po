msgstr "Afbeelding herladen"

#: ../plugins/reload/iog-reload-plugin.c:38
#: ../plugins/reload/reload.iog-plugin.desktop.in.h:2
msgid "Reload current image"
msgstr "Huidige afbeelding herladen"

#: ../plugins/statusbar-date/statusbar-date.iog-plugin.desktop.in.h:1
msgid "Date in statusbar"
msgstr "Datum in statusbalk"

#: ../plugins/statusbar-date/statusbar-date.iog-plugin.desktop.in.h:2
msgid "Shows the image date in the window statusbar"
msgstr "Toont de datum van de afbeelding in de statusbalk"

#: ../src/iog-application.c:124
msgid "Running in fullscreen mode"
msgstr "In volledig scherm"

