<?xml version="1.0" encoding="UTF-8" ?>

<node name="/org/gde2/iog/Iog">

  <interface name="org.gde2.iog.Application">
    <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="iog_application"/>

    <method name="OpenWindow">
      <annotation name="org.freedesktop.DBus.GLib.CSymbol" value="iog_application_open_window"/>
      <arg type="u" name="timestamp" direction="in"/>
      <arg type="y" name="flags" direction="in"/>
    </method>

    <method name="OpenUris">
      <annotation name="org.freedesktop.DBus.Glib.CSymbol" value="iog_application_open_uris"/>
      <arg type="as" name="uris" direction="in"/>
      <arg type="u" name="timestamp" direction="in"/>
      <arg type="y" name="flags" direction="in"/>
    </method>

  </interface>

</node>
